    "def maxProduct(nums):\n",
    "    a = max(nums)\n",
    "    nums.remove(a)\n",
    "    b = max(nums)\n",
    "    print (a*b)\n",
    "    \n",
    "#test#\n",
    "maxProduct([5, 20, 2, 6]) # 得到 120 \n",
    "maxProduct([10, -20, 0, 3]) # 得到 30 \n",
    "maxProduct([-1, 2]) # 得到 -2 \n",
    "maxProduct([-1, 0, 2]) # 得到 0\n"
   ]
